{
  "name": "rster",
  "version": "0.1.0",
  "description": "RSTER is a toolset for REST Api creation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Nicolas Schmidt",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@rster/basic": "^0.1.0",
    "@rster/common": "^0.1.0",
    "@rster/types": "^0.1.0",
    "debug": "^4.3.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.8.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.48.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "echo \"Dependencies are tested and this package just reexports them :)\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "prepare:docs": "cd docs && npm ci && cd ..",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:docs:typedoc": "typedoc",
    "build:docs:docusaurus": "npm run prepare:docs && cd docs && npm run build && cd ..",
    "build:docs": "npm run build:docs:typedoc && npm run build:docs:docusaurus",
    "lint": "eslint . --ext .ts"
  }
}

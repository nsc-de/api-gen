{
  "name": "rster",
  "version": "0.1.0",
  "description": "Toolset for REST Api creation",
  "main": "lib/index.js",
  "author": "Nicolas Schmidt",
  "license": "BSD-2-Clause",
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "debug": "^4.3.4",
    "esm": "^3.2.25",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.8.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-jest": "^29.6.2",
    "cliui": "^8.0.1",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "mongodb": "^5.7.0",
    "source-map-support": "^0.5.21",
    "string-width": "^6.1.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "optionalDependencies": {
    "express": "^4.18.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start-test": "node -r source-map-support/register lib/test.js",
    "test": "jest --config=jest.config.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "prepare:docs": "cd docs && npm ci && cd ..",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/server --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:docs:typedoc": "typedoc",
    "build:docs:docusaurus": "npm run prepare:docs && cd docs && npm run build && cd ..",
    "build:docs": "npm run build:docs:typedoc && npm run build:docs:docusaurus",
    "lint": "eslint . --ext .ts"
  }
}

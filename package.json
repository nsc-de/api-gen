{
  "name": "rster",
  "version": "0.1.0",
  "description": "Toolset for REST Api creation",
  "main": "dist/server/index.js",
  "author": "Nicolas Schmidt",
  "license": "BSD-2-Clause",
  "dependencies": {
    "cross-fetch": "^3.1.6",
    "debug": "^4.3.4",
    "esm": "^3.2.25",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.8.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "chai": "^4.3.7",
    "eslint": "^8.45.0",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.21",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "optionalDependencies": {
    "express": "^4.18.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start-test": "node -r source-map-support/register lib/server/test.js",
    "test": "mocha -r esm --recursive \"dist/**/*.js\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "build": "yarn build:types && yarn build:js && yarn build:test",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:test": "babel test --out-dir dist --extensions \".js,.jsx\" --source-maps inline",
    "build:docs": "typedoc",
    "lint": "eslint . --ext .ts"
  }
}
